<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smartjobs.smartjobs">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.WebviewActivity"></activity>
        <activity android:name=".activity.ContactAgentActivity" />
        <activity android:name=".activity.OnlineAgentActivity" />
        <activity android:name=".activity.AgentInformationActivity" />
        <activity
            android:name=".activity.AgentActivity"
            android:label="@string/title_activity_offerer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.ClientActivity"
            android:label="@string/title_activity_client"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" />
        <activity android:name=".activity.SignupActivity" />
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ResetPasswordActivity" />
        <activity android:name=".activity.ProfileActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
    </application>

</manifest>